# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME="Wine App API"

# Environment
ENV=prod

# Supabase Configuration for production
SUPABASE_URL=https://your-prod-instance.supabase.co
SUPABASE_ANON_KEY=your_production_anon_key_here
SUPABASE_SERVICE_KEY=your_production_service_key_here

# CORS
BACKEND_CORS_ORIGINS=["https://yourdomain.com","https://www.yourdomain.com"]

# JWT
SECRET_KEY=your_production_secret_key_here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=10080  # 7 days

# Firecrawl API key for web scraping
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# API keys for external services
GEMINI_API_KEY=your_gemini_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Logfire API key
LOGFIRE_API_KEY=your_logfire_api_key_here 