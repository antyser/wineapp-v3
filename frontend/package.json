{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "dotenv -e .env -- npx expo start",
    "android": "dotenv -e .env -- npx expo run:android",
    "ios": "dotenv -e .env -- npx expo run:ios",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 pod install",
    "web": "dotenv -e .env -- npx expo start --web",
